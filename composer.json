{
    "name": "saro0h/to-do-list",
    "description": "Openclassroom project 8 by Nicolas Riso",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4.26",
        "doctrine/cache": "^1.11",
        "doctrine/common": "2.13",
        "doctrine/dbal": "^2.13.9",
        "doctrine/doctrine-bundle": "^2.3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "2.7.*",
        "doctrine/persistence": "^1.3.3",
        "doctrine/sql-formatter": "^1.0.1",
        "fzaninotto/faker": "^1.9",
        "incenteev/composer-parameter-handler": "^2.0",
        "liip/test-fixtures-bundle": "^1.9",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "*",
        "symfony/browser-kit": "*",
        "symfony/cache": "5.2.12",
        "symfony/config": "^5.3.0",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/dependency-injection": "*",
        "symfony/doctrine-bridge": "5.1",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.0",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "*",
        "symfony/security-core": "*",
        "symfony/security-csrf": "*",
        "symfony/stopwatch": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "theofidry/alice-data-fixtures": "^1.3",
        "twig/extra-bundle": "*",
        "twig/twig": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "9",
        "symfony/maker-bundle": "^1.15",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/web-profiler-bundle": "5.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
